package br.edu.infnet.apppedido;

import org.springframework.boot.ApplicationArguments;
import org.springframework.boot.ApplicationRunner;
import org.springframework.stereotype.Component;

import br.edu.infnet.apppedido.model.negocio.Sobremesa;

@Component
public class SobremesaLoader implements ApplicationRunner {

	@Override
	public void run(ApplicationArguments args) throws Exception {
		Sobremesa sobremesa = new Sobremesa("Casquinha", 5, 345);
				
		System.out.println("[Sobremesa] Inclus√£o realizada com sucesso: " + sobremesa);
	}
}