package br.edu.infnet.appprojetobloco.model.negocio;

public class Funcionario {
	private String nome;
	private float salario;
	
	public Funcionario(String nome, float salario) {
		this.nome = nome;
		this.salario = salario;
	}
	
	@Override
	public String toString() {
		return nome + ";" + salario;
	}
	
	private float calcularRenda() {
		return salario * 2;
	}
}